[user]
    email = git@mail.jerrington.me
    name = Jacob Errington
[push]
    default = simple
[color]
    ui = auto
[core]
    excludesfile = ~/.gitignore
[alias]
    shove = push -f
    co = checkout
    cop = checkout --patch
    stap = stash --patch
    resp = reset --patch
    ci = commit
    s = status
    b = branch
    l = log --graph --oneline --decorate=short
    la = log --all --graph --oneline --decorate=short
    ls = log --all --graph --oneline --decorate=short --stat
    lss = log --all --graph --decorate=short --stat
    lsp = log --all --graph --decorate=short --stat -p
    psuh = push
    p = push
    rc = rebase --continue
    diff-stash = diff 'stash@{0}^..stash@{0}'
    diff-ci = diff 'HEAD^..HEAD'
    d = diff
    dc = diff --cached
